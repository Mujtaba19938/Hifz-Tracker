      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal",
      "env": {
        "EXPO_PUBLIC_API_BASE_URL": "adequate-vibrancy-production.up.railway.app/api"
      },
      "android": {
        "buildType": "apk"
      }
    },
    "production": {
      "env": {
        "EXPO_PUBLIC_API_BASE_URL": "adequate-vibrancy-production.up.railway.app/api"
      },
      "android": {
        "buildType": "apk"
      }
    },
    "production-aab": {
      "env": {
        "EXPO_PUBLIC_API_BASE_URL": "adequate-vibrancy-production.up.railway.app/api"
      },
      "android": {
        "buildType": "app-bundle"
      }
    }
  },
  "submit": {
    "production": {}
  }
